<!DOCTYPE html>
<html>
<head>
<meta http-equiv="Content-Type" content="text/html; charset=utf-8"/>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>@ViewBag.Title – MyBlog</title>
    @Styles.Render("~/Content/css")
    @Scripts.Render("~/bundles/modernizr")
</head>
<body>
    <header>
        <h1>My Blog</h1>
        <img id="app-icon" src="data:image/png;base64," />
    </header>
    <div id="middle">
        <main>
            @RenderBody()
        </main>
        <aside>
            <ul class="nav navbar-nav">
                <li>@Html.ActionLink("Main", "Index", "Home")</li>
                <li>@Html.ActionLink("Add Feedback", "AddFeedback", "Home")</li>
                <li>@Html.ActionLink("Feedback List", "ViewFeedbacks", "Home")</li>
                <li>@Html.ActionLink("Make Questionnaire", "AddQuestionnaire", "Home")</li>
                <li>@Html.ActionLink("Questionnaire List", "ViewQuestionnaires", "Home")</li>
            </ul>
        </aside>
        <hr />
    </div>
    <footer>
        <p>&copy; @DateTime.Now.Year Serhii Shalikov</p>
        <a href="#">Back to top</a>
    </footer>

    @Scripts.Render("~/bundles/jquery")
    @Scripts.Render("~/bundles/bootstrap")
    @RenderSection("scripts", required: false)
</body>
</html>
